<?xml version="1.0"?>
<package>
  <metadata>
    <id>Shuttle.Core.Configuration</id>
    <version>{semver}</version>
    <authors>Eben Roux</authors>
    <owners>Eben Roux</owners>
    <license type="expression">BSD-3-Clause</license>
    <projectUrl>https://github.com/shuttle/Shuttle.Core.Configuration</projectUrl>
    <icon>images\logo.png</icon>
    <repository type="git" url="https://github.com/Shuttle/Shuttle.Core.Configuration.git" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configuration infrastructure components.</description>
    <releaseNotes />
    <copyright>Copyright (c) {year}, Eben Roux</copyright>
    <tags>shuttle configuration</tags>
    <dependencies>
      <group>
        <dependency id="Shuttle.Core.Logging" version="{Shuttle.Core.Logging-version}" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Shuttle.Core.Logging" version="10.0.1" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>
      <group targetFramework="netcoreapp2.1">
        <dependency id="Shuttle.Core.Logging" version="10.0.1" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\..\..\.media\logo.png" target="images\" />
	<file src="lib\**\*.*" target="lib" />
  </files>
